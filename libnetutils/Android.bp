cc_library_shared {
    name: "libnetutils",
    vendor_available: true,

    srcs: [
        "dhcpclient.c",
        "dhcpmsg.c",
        "ifc_utils.c",
        "packet.c",
    ],

    shared_libs: [
        "libcutils",
        "liblog",
    ],

    cflags: [
        "-Werror",
        "-DMTK_HARDWARE",
        "-Wno-unused-parameter",
    ],

    export_include_dirs: ["include"],
}
